<p>@Title</p>
<div class="row mb-3">
    <div class="col">

        <ul class="list-group">
            @foreach (var item in Items)
            {
                <li class="list-group-item">@item</li>
            }
        </ul>
    </div>
</div>

@code {

    [Parameter] public List<string>? Items { get; set; }
    [Parameter] public string Title { get; set; }
}

